build:
  type: text
  content: |-
    #!/bin/sh

    # make all
    # docker build

    sudo apt install npm --yes 
    curl -o- https://raw.githubusercontent.com/nvm-sh/nvm/v0.39.1/install.sh | bash
deploy:
  type: text
  content: |
    #/bin/sh
    nvm install 16 
    nvm use 16

    npm i  

    npm run build 

    export HOST_IP=${ROOST_CLUSTER_IP}

    docker-compose -f docker-compose.yml up --build -d
test: []
uninstall:
  type: text
  content: |+
    #!/bin/sh

dockerCompose: ''
